{
    "name": "laravellive/poster",
    "description": "Your Package Description here",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "sarthak shrivastava",
        "email": "sarthak@bitfumes.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laravel-notification-channels/facebook-poster": "^1.4",
        "laravel-notification-channels/twitter": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.7@dev",
        "orchestra/database": "^3.7@dev",
        "illuminate/support": "^5.7@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Laravellive\\Poster\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravellive\\Poster\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravellive\\Poster\\PosterServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
